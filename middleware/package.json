  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/express-jwt": "0.0.41",
    "@types/jest": "^24.0.11",
    "@types/multer": "^1.3.7",
    "@types/node": "^11.11.3",
    "@types/request": "^2.48.1",
